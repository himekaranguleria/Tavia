<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Cron:etc/crontab.xsd">
    <group id="subscription_cron"> <!-- Doesn't need to be unique -->
        <job name="subscription_run" instance="Modulebazaar\Firstdataapi\Cron\Reorder" method="execute">
            <schedule>* * * * *</schedule>
        </job>
    </group>
</config>